<center><p><%= link_to "Update new photo", new_photo_path, class: "btn btn-default"%></p>
<center><p><%= link_to "New Multi Photo", new_image_path, class: "btn btn-default"%></p>
<% if @photos == [] %>
<p>Photo ninchen</p>
<% else %>
<% @photos.each do |photo| %>
<p><%= image_tag(photo.photo_url, width: 300, height: 300) %></p>
<p><b><%= photo.category.name %></b></p>
<p><b><%= photo.name %></b></p>
<p><b><%= photo.user.nickname %></b></p>
<p><%= link_to "Like", vote_photo_path(photo.id) %> (<%= @like[photo.id] %>)</p>
<% end %>
<% end %>
</center>
